{
  "name": "sentinel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator",
    "supabase:start": "supabase start -x vector",
    "supabase:stop": "supabase stop",
    "supabase:gen-types": "supabase gen types typescript --local > ./src/types/supabase-generated.types.ts && cp ./src/types/supabase-generated.types.ts ./functions/src/types/supabase-generated.types.ts",
    "supabase:create-db-script": "supabase db diff -f changes",
    "supabase:db-dump": "supabase db dump --local --data-only > supabase/seed.sql",
    "supabase:create-storage-script": "supabase db diff -f storage-changes --schema storage",
    "supabase:create-empty-script": "supabase migration new changes",
    "supabase:serve-functions": "supabase functions serve",
    "supabase:reset-db": "supabase db reset",
    "supabase:update-db": "supabase migration up"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.13.0",
    "@emotion/react": "^11.14.0",
    "@fontsource-variable/figtree": "^5.2.6",
    "@hookform/resolvers": "^4.1.3",
    "@supabase/supabase-js": "^2.49.1",
    "@tiptap/extension-link": "^2.11.7",
    "@tiptap/extension-underline": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/react": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "date-fns": "^4.1.0",
    "fast-deep-equal": "^3.1.3",
    "immer": "^10.1.1",
    "lucide-react": "^0.483.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wouter": "^3.6.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "sass-embedded": "^1.86.3",
    "supabase": "^2.20.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.0.0"
  }
}
